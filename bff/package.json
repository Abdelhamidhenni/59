{
  "name": "sgsatisfaction-bff",
  "version": "0.0.1",
  "description": "SGSatisfaction BFF for PWA version",
  "license": "MIT",
  "author": "Fast-IT",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "format": "prettier --write \"**/*.ts\"",
    "start:dev": "nodemon --config nodemon-dev.json",
    "start": "node dist/src/main.js",
    "test": "jest --passWithNoTests",
    "test:ci": "jest --coverage --ci --forceExit --detectOpenHandles --runInBand",
    "test:coverage": "jest --coverage --passWithNoTests",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:watch": "jest --watch",
    "lint": "eslint --ext ts,js src test"
  },
  "jest": {
    "coverageDirectory": "../coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testEnvironment": "node",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    }
  },
  "dependencies": {
    "typeorm-naming-strategies": "2.0.0",
    "@godaddy/terminus": "^4.2.1",
    "@nestjs/common": "7.4.4",
    "@nestjs/core": "7.4.4",
    "@nestjs/platform-express": "7.4.4",
    "@nestjs/swagger": "^4.6.1",
    "@nestjs/terminus": "7.0.1",
    "@nestjs/typeorm": "7.1.4",
    "@types/cookie-parser": "^1.4.2",
    "aws-sdk": "2.771.0",
    "body-parser": "1.19.0",
    "class-transformer": "0.3.1",
    "class-validator": "0.12.2",
    "cookie-parser": "1.4.5",
    "express-rate-limit": "^5.0.0",
    "helmet": "4.1.1",
    "jsonwebtoken": "8.5.1",
    "pg": "8.4.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "6.6.3",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "0.2.28",
    "typescript": "4.0.3",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@nestjs/testing": "7.4.4",
    "@types/csurf": "1.9.36",
    "@types/express": "4.17.8",
    "@types/express-rate-limit": "5.1.0",
    "@types/helmet": "0.0.48",
    "@types/jest": "26.0.14",
    "@types/jsonwebtoken": "8.5.0",
    "@types/node": "14.11.8",
    "@types/supertest": "2.0.10",
    "@typescript-eslint/eslint-plugin": "4.4.1",
    "@typescript-eslint/parser": "4.4.1",
    "dotenv": "^8.2.0",
    "eslint": "7.11.0",
    "eslint-config-prettier": "6.12.0",
    "eslint-plugin-prettier": "3.1.4",
    "husky": "4.3.0",
    "improved-yarn-audit": "2.3.1",
    "jest": "26.5.3",
    "lint-staged": "10.4.0",
    "nodemon": "2.0.5",
    "prettier": "2.1.2",
    "supertest": "5.0.0",
    "ts-jest": "26.4.1",
    "ts-node": "9.0.0",
    "tsconfig-paths": "3.9.0"
  }
}
